{
  "name": "navegantes",
  "version": "2.0.0",
  "description": "Tienda virtual de productos para mascotas",
  "main": "server.js",
  "scripts": {
    "start": "node back/server.js",
    "dev": "export NODE_ENV=DEVELOPMENT&& nodemon back/server",
    "prod": "export NODE_ENV=PRODUCTION&& nodemon back/server",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false && NODE_OPTIONS=--openssl-legacy-provider && npm install --force --prefix front && npm run build --prefix front"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "cloudinary": "^1.32.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "fs-extra": "^10.1.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.7.2",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.0",
    "nodemailer": "^6.8.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  },
  "author": "yeimi",
